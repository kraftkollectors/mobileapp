{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Kraft Kollectors is requesting access to your photos",
          "cameraPermission": "Kraft Kollectors is requesting access to your camera"
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/photos/logo.png",
          "color": "#FFFFFF",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Kraft Kollectors is requesting access to your location."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "allowBackup": false
          }
        }
      ]
    ],
    "name": "Kraft Kollectors",
    "slug": "kraftkollectors",
    "description": "kraftkollectors mobile app",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/photos/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/photos/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "url": "https://u.expo.dev/125c9891-5d66-4f08-a6cb-9f5781412b72"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "125c9891-5d66-4f08-a6cb-9f5781412b72"
      }
    },
    "ios": {
      "bundleIdentifier": "com.polybotics.kraftkollectors",
      "supportsTablet": false,
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["tel", "http", "https"],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.912930599984-qn00lf228935vjpid346r7ncag6us5hc"
            ]
          }
        ]
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "package": "com.polybotics.kraftkollectors",
      "adapativeIcon": {
        "foregroundImage": "./assets/photos/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "runtimeVersion": "1.0.0"
    }
  }
}
